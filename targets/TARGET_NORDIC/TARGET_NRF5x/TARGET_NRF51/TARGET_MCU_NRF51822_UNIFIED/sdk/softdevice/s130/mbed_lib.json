{
    "name": "softdevice",
    "config": {
        "nrf_enable_logging": {
            "help": "When '1' NRF_LOG_* outputs will be sent to stdout.",
            "value": "0",
            "macro_name": "NRF_LOG_ENABLED"
        }
    },
    "macros": [
        "SOFTDEVICE_PRESENT=1",
        "S130"
    ],
    "target_overrides": {
        "*": {
            "target.features_add": ["BLE"],
            "target.bootloader_img": "hex/s130_nrf51_2.0.0_softdevice.hex"
        }
    }
}
